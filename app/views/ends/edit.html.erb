<h1>Editing end</h1>

<% form_for(@end) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :move_id %><br />
    <%= f.text_field :move_id %>
  </p>
  <p>
    <%= f.label :installation_id %><br />
    <%= f.text_field :installation_id %>
  </p>
  <p>
