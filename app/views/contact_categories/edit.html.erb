<h1>Editing contact_category</h1>

<% form_for(@contact_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @contact_category %> |
<%= link_to 'Back', contact_categories_path %>