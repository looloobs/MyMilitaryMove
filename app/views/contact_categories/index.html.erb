<h1>Listing contact_categories</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @contact_categories.each do |contact_category| %>
  <tr>
    <td><%=h contact_category.name %></td>
    <td><%= link_to 'Show', contact_category %></td>
    <td><%= link_to 'Edit', edit_contact_category_path(contact_category) %></td>
    <td><%= link_to 'Destroy', contact_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New contact_category', new_contact_category_path %>